Index: lede-1701/target/linux/ar71xx/base-files/etc/uci-defaults/03_fix_dnsmasq_dhcp
===================================================================
--- /dev/null
+++ lede-1701/target/linux/ar71xx/base-files/etc/uci-defaults/03_fix_dnsmasq_dhcp
@@ -0,0 +1,30 @@
+#!/bin/sh
+#
+# Copyright (C) 2013 OpenWrt.org
+#
+
+. /lib/functions.sh
+
+config_dnsmasq_dhcp()
+{
+		uci set dhcp.@dhcp[0].force=1
+		uci commit dhcp
+}
+
+
+
+. /lib/ar71xx.sh
+
+board=$(ar71xx_board_name)
+
+case "$board" in
+	gl-ar750ac)
+	config_dnsmasq_dhcp
+	;;
+*)
+	;;
+esac
+
+
+exit 0
+
